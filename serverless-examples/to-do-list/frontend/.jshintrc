{
  "globals": { 
    "$": true,
    "todoItemTpl": true,
    "todoListTpl": true,
    "editTpl": true,
    "addTpl": true,
    "errTpl": true,
    "navBarTpl": true,
    "navBarScheduleTpl": true
  },

  "camelcase": false,
  "curly": true,
  "forin": false,
  "latedef": "nofunc",
  "newcap": false,
  "noarg": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "strict": true,
  "trailing": true,
  "maxlen": 80,

  "eqnull": true,
  "esnext": true,
  "expr": true,
  "globalstrict": true,

  "maxerr": 1000,
  "regexdash": true,
  "laxcomma": true,
  "proto": true,

  "browser": true,
  "devel": true,
  "nonstandard": true,
  "worker": true,

  "-W078": true,
  "predef": ["define", "module"]
}
